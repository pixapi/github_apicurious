<div class="container-fluid container-home">
  <div class="col-md-2"></div>
  <div class="col-md-8">
    <h1 class="home">Followers Activity</h1></br>
    <ul>
      <% @followers_activity.each do |activity| %>
        <h2 class="home">Id: <%= activity.id %></h2>
        <h2 class="home">Type: <%= activity.type %></h2>
        <h2 class="home">Author: <%= activity.author %></h2>
        <h2 class="home">Repo Link: <%= activity.repo_link %></h2>
        </br>
      <% end %>
    </ul>
    <%= link_to "Go back to dashboard", dashboard_path, class: "btn btn-lg btn-default btn-home" %>
    </br></br>
  </div>
  <div class="col-md-2"></div>
</div>
